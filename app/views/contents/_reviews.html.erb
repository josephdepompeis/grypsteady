<div class="modal-body">

<h1>Listing Reviews</h1>

<table>
  <thead>
    <tr>
      <th>Design</th>
      <th>Username</th>
      <th>Note</th>
      <th>Stars</th>
      <th>Size</th>
      <th>Comfort</th>
      <th>Performance</th>
      <th>Gender</th>
      <th>Boolean</th>
      <th>Sport</th>
      <th>Age</th>
      <th>Size purchased</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% design.reviews.each do |review| %>
      <tr>
        <td><%= review.design_id %></td>
        <td><%= review.username %></td>
        <td><%= review.note %></td>
        <td><%= review.stars %></td>
        <td><%= review.size %></td>
        <td><%= review.comfort %></td>
        <td><%= review.performance %></td>
        <td><%= review.gender %></td>
        <td><%= review.boolean %></td>
        <td><%= review.sport %></td>
        <td><%= review.age %></td>
        <td><%= review.size_purchased %></td>
        <td><%= link_to 'Show', review %></td>
        <td><%= link_to 'Edit', edit_review_path(review) %></td>
        <td><%= link_to 'Destroy', review, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>





</div>
<div class="modal-footer">
  <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
</div>
